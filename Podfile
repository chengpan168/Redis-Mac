# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'
 platform :osx, '10.12'

target 'Redis-Mac' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!


  # Pods for Redis-Mac
	pod 'XCGLogger', '~> 4.0.0'
    pod 'SwiftyJSON'
end


target 'Redis-MacTests' do
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    use_frameworks!
    
    # Pods for Redis-OSX
    pod 'XCGLogger', '~> 4.0.0'
    pod 'SwiftyJSON'
end
